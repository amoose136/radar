				<string>import urllib2, json, sys, os
from subprocess import call

[lat,lon] = "{query}".split(' ')
data = urllib2.urlopen("http://forecast.weather.gov/MapClick.php?lat=" + lat + "&amp;lon=" + lon + "&amp;FcstType=json")
ws = json.load(data)['location']['radar'].rstrip() #ws = Weather Station
ws = ws[-3:]
sys.stdout.write('{\"alfredworkflow\":{\"arg\": "'+ws+'","variables":{"ws": "'+ws+'"}}}')
call('/usr/libexec/PlistBuddy -c "Set :variables:ws \"'+ws+'\"" info.plist', shell=True)</string>
